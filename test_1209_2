
public class test_1209_2 {

	public static void main(String[] args) {
		// 1.다음을 한줄의 코드로 작성해라.
		// 1.1Boolean 값 false를 “false”라는 문자열로 변환해라.
		//Boolean b=false;	//오토언박싱
		//b.toString();
		
		String str = Boolean.toString(false);
		System.out.println(str);

		// 1.2 문자열 “36.5”를 double 타입 실수로 변환해라.
		double d=Double.parseDouble("36.5");
		System.out.println(d);

		// 1.3 문자열 “35”를 정수 35로 변환해라.
		int num=Integer.valueOf("35");
		System.out.println(num);
	}

}

import java.util.HashMap;

public class Ex_Hashmap1 {
	public static void main(String[] args) {
		//HashMap<K, V>
		//예) 네이버 로그인에서 ID, 비밀번호는 Map 구조이다..
		
		//set은 add(), map은 put()으로 추가한다.
		HashMap<Integer, String> h=new HashMap<Integer, String>();
		
		h.put(1, "user1");
		h.put(2, "user2");	//삽입
		
		System.out.println(h.get(1));//출력-> user1
		System.out.println(h.get(2));//출력-> user2
	}
}

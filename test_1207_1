
class Point{
	int a,b;
	Point(int a, int b){
		this.a=a;
		this.b=b;
	}
	public String toString() {
		String str=a+","+b;
		return str;
	}
}
public class test_1207_1 {
	public static void main(String[] args) {
		//1. main()를 보고 클래스를 작성해라.
		//main(){
		//Point p = new Point(3,4);
		//System.out.println(p); //3,4출력 

		Point p=new Point(3,4);
		System.out.println(p);
		
		//2. 주민번호 “841111-1234560”을 초기화하여여성인지 남성인지 출력해
		//(7번째 숫자가 1이면 남성, 2이면여성 – switch case사용)
		String str="841111-1234560";
		String str2=str.substring(7,8);
		
		switch(str2) {
		case "1":
			System.out.println("남성");
			break;
		case "2":
			System.out.println("여성");
			break;
		}
		
		//char c=str.charAt(7);
		//switch(str2) {
		//case '1':
		//	System.out.println("남성");
		//	break;
		//case '2':
		//	System.out.println("여성");
		//	break;
		}
	}
